{
  "name": "client2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "test": "exit 0",
    "test:all": "jest",
    "tsc": "tsc",
    "start": "next start",
    "lint": "eslint \"./src/**/*.{js, jsx,ts,tsx}\" --quiet --fix && eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "gen-env": "gen-env-types .env -o src/env.d.ts -e ."
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@headlessui/react": "^1.3.0",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.23.0",
    "formik": "^2.2.9",
    "gen-env-types": "^1.3.0",
    "next": "11.0.1",
    "next-images": "^1.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.6",
    "jest-junit": "^12.2.0",
    "jest-resolve": "^27.0.6",
    "jest-serializer-html": "^7.1.0",
    "jest-watch-typeahead": "^0.6.4",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.3.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "dotenv/config"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.(spec|test).{js,jsx,ts,tsx}",
      "<rootDir>/test/**/*.(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/../abb-common/test/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/../abb-common/test/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@frontend/(.*)$": "<rootDir>/src/$1",
      "^react$": "<rootDir>/node_modules/react",
      "^formik$": "<rootDir>/node_modules/formik",
      "^@common/(.*)$": "<rootDir>/../abb-common/src/$1",
      "^@common-test/(.*)$": "<rootDir>/../abb-common/test/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "<rootDir>/node_modules/jest-watch-typeahead/filename.js",
      "<rootDir>/node_modules/jest-watch-typeahead/testname.js"
    ],
    "resetMocks": true,
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  },
  "jest-junit": {
    "ancestorSeparator": " â€º ",
    "suiteName": "Frontend Jest tests",
    "outputName": "junit-frontend.xml"
  }
}
