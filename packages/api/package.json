  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "cross-env NODE_ENV=development nodemon dist/index.js",
    "start": "NODE_ENV=production node dist/index.js",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "deploy": "./scripts/deploy.sh",
    "db:setup": "ts-node ./src/test-utils/setup.ts",
    "test": "cross-env NODE_ENV=test npm run db:setup && jest",
    "test:watch": "cross-env NODE_ENV=test npm run db:setup && jest --watch",
    "gen:migration": "typeorm migration:generate -n <NAME>",
    "run:migration": "rm -rf dist && npm run build && typeorm migration:run"
  },
