{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react-native",
    "lib": [
      "es6",
      "esnext.asynciterable"
    ],
    "paths": {
      "@src/*": ["src/*"],
      "@common/*": ["../abb-common/src/*"]
    },
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "rootDir": "./src",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "baseUrl": "."
  }
}