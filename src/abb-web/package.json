{
  "name": "abb-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "test": "exit 0",
    "test:all": "jest",
    "start": "next start",
    "lint": "eslint \"./src/**/*.{js, jsx,ts,tsx}\" --quiet --fix && eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.4",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fontsource/lato": "^4.4.5",
    "@urql/exchange-graphcache": "^4.0.0",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.4",
    "date-fns": "^2.22.1",
    "formik": "^2.2.6",
    "framer-motion": "^4.1.17",
    "gen-env-types": "^1.3.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "next": "11.0.1",
    "next-apollo": "^5.0.4",
    "next-images": "^1.8.1",
    "next-urql": "^3.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-is": "^17.0.2",
    "sass-loader": "^12.1.0",
    "urql": "^2.0.2",
    "wonka": "^4.0.15"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@graphql-codegen/cli": "1.21.7",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@graphql-codegen/typescript-urql": "^2.0.6",
    "@testing-library/dom": "^8.0.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.4",
    "@types/react-leaflet": "^2.8.1",
    "@types/react-places-autocomplete": "^7.2.7",
    "@types/testing-library__dom": "^7.5.0",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/testing-library__react": "^10.2.0",
    "@types/testing-library__react-hooks": "^4.0.0",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^27.0.2",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.4",
    "jest-junit": "^12.0.0",
    "jest-resolve": "^27.0.4",
    "jest-serializer-html": "^7.0.0",
    "jest-watch-typeahead": "^0.6.3",
    "leaflet": "^1.7.1",
    "polished": "^4.1.2",
    "prettier": "^2.2.1",
    "react-leaflet": "^3.2.0",
    "sass": "^1.34.1",
    "typescript": "^4.2.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "dotenv/config"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.(spec|test).{js,jsx,ts,tsx}",
      "<rootDir>/test/**/*.(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/../abb-common/test/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/../abb-common/test/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@frontend/(.*)$": "<rootDir>/src/$1",
      "^react$": "<rootDir>/node_modules/react",
      "^formik$": "<rootDir>/node_modules/formik",
      "^@common/(.*)$": "<rootDir>/../abb-common/src/$1",
      "^@common-test/(.*)$": "<rootDir>/../abb-common/test/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "<rootDir>/node_modules/jest-watch-typeahead/filename.js",
      "<rootDir>/node_modules/jest-watch-typeahead/testname.js"
    ],
    "resetMocks": true,
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  },
  "jest-junit": {
    "ancestorSeparator": " â€º ",
    "suiteName": "Frontend Jest tests",
    "outputName": "junit-frontend.xml"
  }
}
