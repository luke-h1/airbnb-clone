{
  "name": "airbnb-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "tsc": "tsc",
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "lint": "eslint \"./src/**/*.{js, jsx,ts,tsx}\" --quiet --fix && eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "gen": "apollo client:codegen --target typescript --localSchemaFile schema.gql --outputFlat --includes \"src/**\" --excludes \"src/generated/**\" src/generated",
    "gen:watch": "npx gen --watch",
    "db:init": "npx prisma init",
    "db:format": "npx prisma format",
    "db:generate": "npx prisma generate",
    "db:migrate:new": "npx prisma migrate dev",
    "db:migrate:up": "npx prisma migrate deploy",
    "db:migrate:down": "npx prisma migrate down"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@babel/core": "^7.15.0",
    "@prisma/client": "^2.30.2",
    "@reach/combobox": "^0.11.2",
    "apollo-server-micro": "^2.19.0",
    "autoprefixer": "^10.0.2",
    "class-validator": "^0.12.2",
    "cloudinary": "^1.23.0",
    "cloudinary-react": "^1.6.7",
    "cookie": "^0.4.1",
    "deepmerge": "^4.2.2",
    "firebase": "^8.0.2",
    "firebase-admin": "^9.4.1",
    "gen-env-types": "^1.3.0",
    "geolib": "^3.3.1",
    "graphql": "^15.4.0",
    "js-cookie": "^2.2.1",
    "next": "11.1.0",
    "postcss": "^8.1.7",
    "postcss-import": "^14.0.0",
    "prisma": "^2.30.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebaseui": "^4.1.0",
    "react-hook-form": "^6.11.1",
    "react-map-gl": "^5.2.10",
    "reflect-metadata": "^0.1.13",
    "tailwindcss": "^2.0.1",
    "type-graphql": "^1.1.1",
    "use-debounce": "^5.0.4",
    "use-google-maps-script": "^0.1.0",
    "use-places-autocomplete": "^1.6.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@types/cookie": "^0.4.1",
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^16.9.56",
    "@types/react-map-gl": "^5.2.9",
    "apollo": "^2.31.1",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "postcss-flexbugs-fixes": "^5.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "typescript": "^4.4.2"
  },
  "resolutions": {
    "graphql": "15.4"
  }
}
